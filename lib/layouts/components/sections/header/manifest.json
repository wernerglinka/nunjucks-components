{
  "name": "header",
  "type": "section",
  "styles": ["header.css"],
  "scripts": ["header.js"],
  "requires": ["branding", "navigation", "page-transitions", "language-switcher"],
  "context": ["urlPath", "data.languages"],
  "validation": {
    "required": ["sectionType"],
    "properties": {
      "sectionType": {
        "type": "string",
        "const": "header"
      },
      "isDisabled": {
        "type": "boolean"
      },
      "containerFields.inContainer": {
        "type": "boolean"
      },
      "containerFields.isAnimated": {
        "type": "boolean"
      },
      "containerFields.noMargin.top": {
        "type": "boolean"
      },
      "containerFields.noMargin.bottom": {
        "type": "boolean"
      },
      "containerFields.noPadding.top": {
        "type": "boolean"
      },
      "containerFields.noPadding.bottom": {
        "type": "boolean"
      },
      "containerFields.background.isDark": {
        "type": "boolean"
      }
    }
  }
}
